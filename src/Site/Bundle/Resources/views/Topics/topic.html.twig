{% extends "CoreBundle::layout.html.twig" %}

{% block title "MarwolfGaming - " ~ name.subject %}

{% block content %}
    <h1>{{ name.subject }}!</h1>
	<div id="posts-list">
		{% for post in posts %}
		<div class="post" id="post_{{ post.id }}">
			<div class="post_content">
				{{ post.content }}
			</div>
		</div>
		{% endfor %}
	</div>
{% endblock %}

